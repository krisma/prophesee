<h1>Prophesee</h1>
<div id="loginbox">
	<% if user_signed_in? %>
	<%= render 'signed_in' %>
	<% else %>
	<%= render 'devise/sessions/new' %>

	<% end %>

</div>
<div id="postbox">
	<%= render 'posts/form' %>
</div>
<div id="main">
	<h5>Most recent prophesees</h5>
	<% @posts.each do |p| %>
		<p><%= p.content %>, <%= p.post_created %></p>
	<% end %>
</div>